{
	"name": "Gluon Dark",
	"colors": {
		"foreground": "#FFF",
		"disabledForeground": "#444",

		"editor.background": "#000",
		"editor.foreground": "#EEE"
	},
	"tokenColors": [
		{
			"name": "Comment Color",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"constant.character"
			],
			"settings": {
				"foreground": "#0F0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Number Color",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#0FF"
			}
		},
		{
			"name": "Built-in Function Color",
			"scope": [
				"support",
				"entity",
				"support.function"
			],
			"settings": {
				"foreground": "#07F"
			}
		},
		{
			"name": "Operator Color",
			"scope": [
				"keyword.operator",
				"punctuation.separator",
				"punctuation.terminator",
				"support.constant"
			],
			"settings": {
				"foreground": "#FF0"
			}
		},
		{
			"name": "Keyword Color",
			"scope": [
				"variable.language",
				"storage",
				"storage.modifier",
				"keyword",
				"constant.language"
			],
			"settings": {
				"foreground": "#F00"
			}
		},
		{
			"name": "String Color",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#F0F",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Parameter Color",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#F80",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Error Color",
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#F00"
			}
		}
	]
}