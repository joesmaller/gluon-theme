{
	"name": "Gluon Dark High Contrast",
	"colors": {
		"focusBorder": "#FFF",
		"foreground": "#FFF",
		"disabledForeground": "#444",
		"selection.background": "#F00",

		"editor.background": "#000",
		"editor.foreground": "#FFF",

		"contrastActiveBorder": "#FFF",
		"contrastBorder": "#888",

		"toolbar.hoverOutline": "#F00",

		"editorLineNumber.foreground": "#666",
		"editorLineNumber.activeForeground": "#FFF",
		"editorLineNumber.dimmedForeground": "#444",

		"editorCursor.background": "#000",
		"editorCursor.foreground": "#FFF",

		"editor.selectionBackground": "#FFF",
		"editor.selectionForeground": "#000",

		"editor.lineHighlightBackground": "#FFF0",
		"editor.lineHighlightBorder": "#888",

		"editor.guides.indentation": true,
		"editorWhitespace.foreground": "#8888",

		"editorBracketMatch.background": "#8884",
		"editorBracketHighlight.foreground1": "#FF0",
		"editorBracketHighlight.foreground2": "#F0F",
		"editorBracketHighlight.foreground3": "#FF0"
	},
	"tokenColors": [
		{
			"name": "Comment Color",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"constant.character"
			],
			"settings": {
				"foreground": "#0F0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Number Color",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#0FF"
			}
		},
		{
			"name": "Built-in Function Color",
			"scope": [
				"support",
				"entity",
				"support.function"
			],
			"settings": {
				"foreground": "#08F"
			}
		},
		{
			"name": "Operator Color",
			"scope": [
				"keyword.operator",
				"punctuation.separator",
				"punctuation.terminator",
				"support.constant"
			],
			"settings": {
				"foreground": "#FF0"
			}
		},
		{
			"name": "Keyword Color",
			"scope": [
				"variable.language",
				"storage",
				"storage.modifier",
				"keyword",
				"constant.language"
			],
			"settings": {
				"foreground": "#F00"
			}
		},
		{
			"name": "String Color",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#F0F",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Function Parameter Color",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#F80"
			}
		},
		{
			"name": "Error Color",
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#F00"
			}
		}
	]
}